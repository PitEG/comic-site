server {
  listen       80 default_server;
  listen  [::]:80 default_server;
  server_name  _;

  root /var/www/html;

  # Add index.php to the list if you are using PHP
  index index.html index.nginx-debian.html;

  server_name _;

  location / {
    try_files $uri $uri/ =404;
  }

  # location /comics {
  #   try_files $uri /comics/index.html;
  # }
}

